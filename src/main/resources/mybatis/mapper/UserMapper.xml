<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.springbootmybatis.multi.UserMapper">
    <select id="getUserId" resultType="com.example.springbootmybatis.model.UserInfo">
        select * from users
    </select>
    <select id="getUserList" resultType="com.example.springbootmybatis.model.UserInfo">
        select * from users where d_id=10
    </select>
</mapper>